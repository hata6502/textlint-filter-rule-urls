{
  "name": "textlint-filter-rule-urls",
  "version": "1.0.1",
  "description": "textlint filter rule that ignore reported errors at URLs. ",
  "keywords": [
    "textlint",
    "filter",
    "rule"
  ],
  "repository": "https://github.com/hata6502/textlint-filter-rule-urls",
  "license": "MIT",

  },
  "devDependencies": {
    "@textlint/ast-node-types": "^4.2.5",
    "@textlint/types": "^1.3.1",
    "@types/node": "^14.0.22",
    "license-checker": "^25.0.1",
    "typescript": "^4.1.2"
